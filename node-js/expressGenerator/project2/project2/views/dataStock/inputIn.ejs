<%- include ('../_header') %>

<a href="/dataStock" class="btn btn-primary">tabel</a>
<%- include ('../_message') %>

<form action="/dataStock/input" method="POST">
  <div class="form-group">
    <label for="kode">Kode</label>
    <input
      type="text"
      id="kode"
      name="kode"
      class="form-control"
    />
  </div>
  <div class="form-group">
    <label for="nama">Nama Barang</label>
    <input
      type="text"
      id="nama"
      name="nama"
      class="form-control"
    />
  </div>
  <div class="form-group">
    <label for="total_in">Total Masuk</label>
    <input
      type="text"
      id="total_in"
      name="total_in"
      class="form-control"
    />
  </div>
  <div class="form-group">
    <label for="date_in">Tanggal Masuk</label>
    <input
      type="date"
      id="date_in"
      name="date_in"
      class="form-control"
    />
  </div>
  <div class="form-group">
    <label for="no_invoice">No Invoice</label>
    <input
      type="text"
      id="no_invoice"
      name="no_invoice"
      class="form-control"
    />
  </div>
  <div class="form-group">
    <label for="harga_satuan">Harga Satuan</label>
    <input
      type="number"
      id="keterangan"
      name="keterangan"
      class="form-control"
    />
  </div>
  <button type="submit" class="btn btn-primary btn-block">
    Add Data
  </button>
</form>
