<%- include ('../_header') %>

<table class="table table-sm" border="2">
      <tr>
        <th>KODE BARANG</th>
        <th>NAMA BARANG</th>
        <th>TOTAL MASUK</th>
        <th>TANGGAL MASUK</th>
        <th>NO INVOICE</th>
        <th>KETERANGAN</th>

      </tr>
      <% for (let data of liststock) { %>

      <tr>
        <td><%= data.kode %></td>
        <td><%= data.nama %></td>
        <td><%= data.total_in %></td>
        <td><%= data.date_in %></td>
        <td><%= data.no_invoice %></td>
        <td><%= data.keterangan %></td>
        <td><a href="/dataStock/delete/<%= data.id %>" class="btn btn-danger">delete</a>
        </td>
      </tr>
      <% } %>

    </table>
  